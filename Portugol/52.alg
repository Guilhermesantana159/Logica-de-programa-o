algoritmo "semnome"
var
V,Y,X,SOMA,DEZO,CINCUN,MAIOR:REAL
inicio
V<-0
Y<-5
ENQUANTO V<=Y FACA
ESCREVAL("DIGITE SUA IDADE:")
LEIA(X)
LIMPATELA
V<-V+1
SOMA<-SOMA+X
SE X<5 ENTAO
CINCUN<-CINCUN+1
FIMSE
SE X>18 ENTAO
DEZO<-DEZO+1
FIMSE
SE X>MAIOR ENTAO
MAIOR<-X
FIMSE
FIMENQUANTO
ESCREVAL("A MEDIA DA IDADE FOI:",(SOMA/Y))
ESCREVAL("TEM MAIS DE 18 ANOS",DEZO)
ESCREVAL("TEM MENOS DE 5 ANOS",CINCUN)
ESCREVAL("A MAIOR IDADE FOI:",MAIOR)
fimalgoritmo

