algoritmo "semnome"
var
X,Y,Z,RES:INTEIRO
FUNCAO MAIOR(A,B,C:INTEIRO):INTEIRO
VAR
MAIO:INTEIRO
INICIO
SE(A>B)E(A>C) ENTAO
MAIO<-A
SENAO
     SE(B>A)E(B>C)ENTAO
     MAIO<-B
     SENAO
     MAIO<-C
     FIMSE
FIMSE
RETORNE MAIO
fimfuncao
inicio
ESCREVAL("DIGITE O PRIMEIRO VALOR:")
LEIA(X)
ESCREVAL("DIGITE O SEGUNDO VALOR:")
LEIA(Y)
ESCREVAL("DIGITE O SEGUNDO VALOR:")
LEIA(Z)
RES<-MAIOR(X,Y,Z)
ESCREVAL("O MAIOR NUMERO DIGITADO FOI ",RES)
fimalgoritmo

