algoritmo "semnome"
var
X,Y:INTEIRO
PROCEDIMENTO MAIOR(A,B:INTEIRO)
INICIO
SE A>B ENTAO
ESCREVAL("O VALOR ",A," E MAIOR QUE ",B)
SENAO
     SE B>A ENTAO
     ESCREVAL("O VALOR ",B," E MAIOR QUE ",A)
     SENAO
     ESCREVAL("OS VALORES SAO IGUAIS")
     FIMSE
FIMSE
FIMPROCEDIMENTO
inicio
ESCREVAL("DIGITE O PRIMEIRO VALOR:")
LEIA(X)
ESCREVAL("DIGITE O SEGUNDO VALOR:")
LEIA(Y)
MAIOR(X,Y)

fimalgoritmo
