algoritmo "semnome"
var
Y,Z,N:REAL
X:CARACTERE
inicio

ESCREVAL("O SEU NOME:")
LEIA(X)
ESCREVAL("DIGITE O SALARIO:")
LEIA(Y)
ESCREVAL("DIGITE QUANTOS ANO VOCE JA TRABALHA NA EMPRESA:")
LEIA(Z)
SE(Z<=3)ENTAO
N<-(Y*1.03)
SENAO
SE(Z>=3) E (Z<=10)ENTAO
N<-(Y*1.125)
SENAO
N<-(Y*1.20)
FIMSE
FIMSE
ESCREVAL("O SALARIO DO ",X," SERA ",N )

fimalgoritmo
