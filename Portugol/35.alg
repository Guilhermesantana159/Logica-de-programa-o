algoritmo "semnome"
var
X:CARACTERE
M,V,Q,Z:REAL
inicio
ESCREVAL("QUAL O TIPO DE CARRO ALUGADO:")
LEIA(X)
ESCREVAL("QUANTOS KM PERCORRIDO:")
LEIA(M)
ESCREVAL("QUANTOS DIAS TEVE O CARRO ALUGADO")
LEIA(V)

SE X="POPULAR" ENTAO
Q<-90*V+Z
SE M>100 ENTAO
Z<-(M-100)*0.10+20
SENAO
Z<-M*0.20
FIMSE
FIMSE
SE X="LUXO" ENTAO
Q<-150*V+Z
SE M>200 ENTAO
Z<-(M-200)*0.25+60
SENAO
Z<-M*0.30
FIMSE
FIMSE
ESCREVAL("O PRECO PAGO SERA ",Q)
fimalgoritmo
